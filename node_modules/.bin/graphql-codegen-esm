else
  export NODE_PATH="/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/@graphql-codegen+cli@3.0.0_@babel+core@7.21.0_@swc+core@1.3.36_@types+node@18.14.0_graphql@16.6.0_typescript@5.0.4/node_modules/@graphql-codegen/cli/esm/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/@graphql-codegen+cli@3.0.0_@babel+core@7.21.0_@swc+core@1.3.36_@types+node@18.14.0_graphql@16.6.0_typescript@5.0.4/node_modules/@graphql-codegen/cli/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/@graphql-codegen+cli@3.0.0_@babel+core@7.21.0_@swc+core@1.3.36_@types+node@18.14.0_graphql@16.6.0_typescript@5.0.4/node_modules/@graphql-codegen/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/@graphql-codegen+cli@3.0.0_@babel+core@7.21.0_@swc+core@1.3.36_@types+node@18.14.0_graphql@16.6.0_typescript@5.0.4/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/node_modules:$NODE_PATH"
