#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/ts-node@10.9.1_@swc+core@1.3.36_@types+node@18.14.0_typescript@5.0.4/node_modules/ts-node/dist/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/ts-node@10.9.1_@swc+core@1.3.36_@types+node@18.14.0_typescript@5.0.4/node_modules/ts-node/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/ts-node@10.9.1_@swc+core@1.3.36_@types+node@18.14.0_typescript@5.0.4/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/ts-node@10.9.1_@swc+core@1.3.36_@types+node@18.14.0_typescript@5.0.4/node_modules/ts-node/dist/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/ts-node@10.9.1_@swc+core@1.3.36_@types+node@18.14.0_typescript@5.0.4/node_modules/ts-node/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/ts-node@10.9.1_@swc+core@1.3.36_@types+node@18.14.0_typescript@5.0.4/node_modules:/home/magister/Documents/DeepSquare/the-grid/customer-portal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../../ts-node/dist/bin-transpile.js" "$@"
fi
