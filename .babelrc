{
  "presets": [
    "next/babel"
  ],
  "env": {
    "production": {
      "plugins": [
        "babel-plugin-react-remove-properties"
      ]
    },
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "babel-plugin-transform-typescript-metadata",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}
